{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@gregrickaby/acf-blocks",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "NodeNext",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "es6",
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": ["**.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
