{
  "name": "gregrickaby/acf-blocks",
  "description": "Custom ACF Blocks for WordPress.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "homepage": "https://gregrickaby.com",
  "authors": [
    {
      "name": "Greg Rickaby",
      "email": "greg@gregrickaby.com"
    }
  ],
  "repositories": [],
  "autoload": {
    "psr-4": {
      "Grd\\Acf\\Blocks\\": "src/classes"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "process-timeout": 600,
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "composer/installers": "^1.11",
    "phospr/fraction": "dev-master"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-cli/wp-cli-bundle": "^2.5",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "scripts": {
    "format": "phpcbf --standard=.phpcs.xml",
    "lint": "phpcs --standard=.phpcs.xml"
  }
}
