{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "acf/gallery",
  "title": "ACF Gallery Block",
  "description": "A block powered by ACF's gallery field and Fancybox.",
  "version": "1.12.0",
  "category": "theme",
  "icon": "format-gallery",
  "textdomain": "acf-blocks",
  "script": "file:./gallery.ts",
  "style": "file:./gallery.css",
  "keywords": ["acf", "custom", "gallery"],
  "acf": {
    "blockVersion": 2,
    "mode": "auto",
    "renderTemplate": "../../plugins/grd-photo-gallery/src/blocks/gallery/gallery.php",
    "postTypes": ["post", "page"]
  },
  "supports": {
    "anchor": false,
    "align": false,
    "className": false,
    "customClassName": true
  }
}
